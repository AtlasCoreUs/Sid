{
  "buildCommand": "prisma generate && next build",
  "framework": "nextjs",
  "installCommand": "npm install",
  "regions": ["cdg1"],
  "functions": {
    "app/api/ai/chat/route.ts": {
      "maxDuration": 30
    },
    "app/api/apps/generate/route.ts": {
      "maxDuration": 60
    }
  },
  "env": {
    "ENABLE_EXPERIMENTAL_COREPACK": "1"
  },
  "crons": [
    {
      "path": "/api/cron/cleanup",
      "schedule": "0 2 * * *"
    }
  ]
}